let $words := .//para =!> normalize-space() =!> tokenize() =!> replace('\(|\)|\.|,|"','')
let $lcase := $words =!> lower-case()
return
map:build(distinct-values($words),
   identity#1,
   fn ($w) {let $lc := lower-case($w) return count($lcase[. eq $lc])}
),
map:build(items-at(.//para,26 to 33),
   fn ($p,$pos) { $pos },
   fn ($p) {normalize-space($p) => tokenize()}
)